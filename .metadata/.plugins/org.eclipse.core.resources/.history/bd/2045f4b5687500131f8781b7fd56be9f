function initialize() {
        var mapOptions = {
          center: new google.maps.LatLng(38.8951100	, -77.0363700),
          zoom: 3
        };
        var map = new google.maps.Map(document.getElementById("woodlawn_map"), mapOptions);
        var location = new google.maps.LatLng(38.8951100, -77.0363700);
        var image = 'Images/W_pin.png';

        var marker = new google.maps.Marker({
            position: location,
            map: map,
            icon: image,
            title:"Hello World!"
        });

        var infowindow = new google.maps.InfoWindow({
        	  content:"Hello World!"
       	});

        google.maps.event.addListener(marker, 'click', function() {
        	  infowindow.open(map,marker);
        });
      }

      